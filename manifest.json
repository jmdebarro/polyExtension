{
    "name": "polyExtention",
    "version": "1.0",
    "manifest_version": 2,
    "permissions": [
      "activeTab",
      "storage"
    ],
    "content_scripts": [
      {
        "matches": ["<all_urls>"],
        "js": ["content.js"]
      }
    ],
    "browser_action": {
      "default_icon": "icon.png",
      "default_popup": "popup.html"
    },
    "background": {
      "scripts": ["background.js"]
    }
  }
  