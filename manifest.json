{
  "name": "polyExtention",
  "version": "1.0",
  "manifest_version": 3,
  "description" : "An extension to display professor's polyratings",
  "permissions": ["activeTab", "storage"],
  "icons" : {
    "16" : "icon16.png"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ],
  "action" : {
    "default_popup" : "index.html"
  }
}
